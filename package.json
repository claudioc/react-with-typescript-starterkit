  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "boilerplate"
  ],
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress --debug --config ./webpack.config.js",
    "build": "NODE_ENV=production webpack",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/src/.*spec)\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "@types/react": "^15.0.26",
    "@types/react-dom": "^15.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "@types/jest": "^19.2.4",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "html-webpack-include-assets-plugin": "0.0.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.5",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.6",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.5"
